unit HistoricoUnit;

interface

type
  Historico = Class
  private
  public
    jogador: String;
    data: String;
    tempo: Integer;
    pont_pre: Integer;
    pont_pos: Integer;
    constructor Historico();
  end;

implementation

{ Historico }

constructor Historico.Historico;
begin
  inherited;
  jogador := '';
  data := '';
  tempo := 0;
  pont_pre := 0;
  pont_pos := 0;
end;

end.
